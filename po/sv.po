# ../lisp/customize.jl 
msgid "Invoke the user-customization system."
msgstr "Anropa systemet för användarkonfiguration."

# ../lisp/cycle.jl 
msgid "Select the next window of the current workspace."
msgstr "Välj nästa fönster på det nuvarande skrivbordet."

# ../lisp/cycle.jl 
msgid "Focus on the previous window of the current workspace."
msgstr "Fokusera det tidigare fönstret på det nuvarande skrivbordet."

# ../lisp/cycle.jl 
msgid "Select the next window, cycling through all possible workspaces."
msgstr "Välj nästa fönster, växla mellan fönster på alla skrivbord."

# ../lisp/cycle.jl 
msgid "Select the previous window, cycling through all possible workspaces."
msgstr "Välj det tidigare fönstret, växla mellan fönster på alla skrivbord."

# ../lisp/edge-flip.jl 
msgid "Edge Flipping"
msgstr "Kantvändning"

# ../lisp/edge-flip.jl 
msgid "Flip to next viewport/workspace when pointer hits screen edge."
msgstr "Hoppa till nästa vy/skrivbord när pekaren träffar skärmkanten."

# ../lisp/edge-flip.jl 
msgid "What hitting the screen edge actually flips."
msgstr "Vad man faktiskt hoppar till."

# ../lisp/edge-flip.jl 
msgid ""
"Number of milliseconds to wait after pointer hits screen edge before\n"
"flipping."
msgstr "Antal millisekunder att vänta efter att pekaren träffat skärmkanten."

# ../lisp/focus.jl 
msgid "When does the mouse pointer affect the input focus."
msgstr "När påverkar muspekaren inmatningsfokus."

# ../lisp/focus.jl 
msgid "Does click-to-focus mode pass the click through to the window."
msgstr "Skickar 'klicka-för-fokus'-läget vidare klicket till fönstret."

# ../lisp/frames.jl 
msgid ""
"Alist of (CLASS . ALIST) associating classes of frame parts with state\n"
"they inherit."
msgstr ""
"Alist av (CLASS . ALIST) som binder klasser med ramdelar till läget\n"
"de ärver."

# ../lisp/frames.jl 
msgid ""
"Alist of (CLASS . ALIST) associating classes of frame parts with state\n"
"that overrides settings set elsewhere."
msgstr ""
"Alist av (CLASS . ALIST) som binder klasser av ramdelar med lägen\n"
"som tar över inställningar satta någon annan stans."

# ../lisp/frames.jl 
msgid "Default frame style (theme)."
msgstr "Standard-ramstil (tema)."

# ../lisp/frames.jl 
msgid "Update all windows when the default frame style is changed."
msgstr "Uppdatera alla fönster när standard-ramstilen ändras."

# ../lisp/frames.jl 
msgid "Decorate transient windows similarly to top-level windows."
msgstr "Dekorerar tillfälliga fönster likadant som vanliga fönster."

# ../lisp/frames.jl 
msgid "Automatically reload themes when they are updated."
msgstr "Ladda automatiskt om teman när de uppdateras."

# ../lisp/frames.jl 
msgid "Number of seconds between checking if theme files have been modified."
msgstr "Antal sekunder mellan kontroll av förändring av temafiler."

# ../lisp/frames.jl 
msgid "Directory containing user-local themes."
msgstr "Katalog som innehåller användarens teman."

# ../lisp/frames.jl 
msgid "Directory containing themes from the current sawmill version."
msgstr "Katalog som innehåller teman från den nuvarande Sawmill-versionen."

# ../lisp/frames.jl 
msgid "Directory containing system-wide themes."
msgstr "Katalog som innehåller teman likadana för systemet."

# ../lisp/frames.jl 
msgid "List of directories from which themes may be loaded."
msgstr "Lista på kataloger från vilka teman kan laddas."

# ../lisp/frames.jl 
msgid "List of (NAME . FUNCTION) defining all loaded frame styles."
msgstr "Lista av (NAMN . FUNKTION) som definierar alla laddade ramstilar."

# ../lisp/frames.jl 
msgid "When non-nil themes are assumed to be malicious."
msgstr "När icke-noll så antas teman vara farliga."

# ../lisp/frames.jl 
msgid "Default"
msgstr "Standard"

# ../lisp/functions.jl 
msgid "Give focus to windows even when they haven't asked for it."
msgstr "Ge fokus till fönster även när de inte har frågat efter det."

# ../lisp/functions.jl 
msgid ""
"Offset in percent from left window edge, when warping.\n"
"A negative number means warp to outside the left window edge."
msgstr ""
"Avstånd i procent från vänstra fönsterkanten vid teleportering. Ett\n"
"negativt tal betyder teleportera till utanför den vänstra fönsterkanten."

# ../lisp/functions.jl 
msgid ""
"Offset in percent from top window edge, when warping.\n"
"A negative number means warp to outside the top window edge."
msgstr ""
"Avstånd i procent från översta fönsterkanten vid teleportering. Ett\n"
"negativt tal betyder teleportera till utanför den övre fönsterkanten."

# ../lisp/functions.jl 
msgid "Start a new xterm."
msgstr "Starta en ny xterm."

# ../lisp/functions.jl 
msgid "Delete the window."
msgstr "Stäng fönstret."

# ../lisp/functions.jl 
msgid "Delete the window, or beep if the window can't be closed safely."
msgstr "Stäng fönstret, eller pip om fönstret inte kan stängas säkert."

# ../lisp/gnome-commands.jl 
msgid "Toggle the GNOME SKIP_WINLIST hint of the window."
msgstr "Växla fönstrets GNOME SKIP_WINLIST-inställning."

# ../lisp/gnome-commands.jl 
msgid "Set the GNOME SKIP_WINLIST hint of the window."
msgstr "Sätt på fönstrets GNOME SKIP_WINLIST-inställning."

# ../lisp/gnome-commands.jl 
msgid "Unset the GNOME SKIP_WINLIST hint of the window."
msgstr "Stäng av fönstrets GNOME SKIP_WINLIST-inställning."

# ../lisp/gnome-commands.jl 
msgid "Toggle the GNOME SKIP_TASKLIST hint of the window."
msgstr "Växla fönstrets GNOME SKIP_TASKLIST-inställning."

# ../lisp/gnome-commands.jl 
msgid "Set the GNOME SKIP_TASKLIST hint of the window."
msgstr "Sätt på fönstrets GNOME SKIP_TASKLIST-inställning."

# ../lisp/gnome-commands.jl 
msgid "Unset the GNOME SKIP_TASKLIST hint of the window."
msgstr "Stäng av fönstrets GNOME SKIP_TASKLIST-inställning."

# ../lisp/gnome-menu.jl 
msgid "Language code used when constructing GNOME menus."
msgstr "Språkkod som används vid konstruktion av GNOME-menyer."

# ../lisp/gnome-menu.jl 
msgid "List of directories to read GNOME menu entries from."
msgstr "Lista på kataloger att läsa GNOME menyalternativ från."

# ../lisp/group-funs.jl 
msgid "Group transient windows with their parents."
msgstr "Gruppera tillfälliga fönster tillsammans med deras föräldrar."

# ../lisp/group-funs.jl 
msgid "List of group ids that always exist, even when they have no members."
msgstr ""
"Lista av grupp-idn som alltid existerar, även när de inte har\n"
"några medlemmar."

# ../lisp/group-funs.jl 
msgid "New group"
msgstr "Ny grupp"

# ../lisp/gtkrc.jl 
msgid "Reload the gtkrc settings."
msgstr "Ladda om gtkrc-inställningarna."

# ../lisp/iconify.jl 
msgid "Iconify the window."
msgstr "Ikonifiera fönstret."

# ../lisp/iconify.jl 
msgid "Return the window from its iconified state."
msgstr "Återställ fönstret från ikonifierat läge."

# ../lisp/iconify.jl 
msgid "Display the workspace containing the window W, then focus on W."
msgstr "Visa skrivbordet som innehåller fönstret W, fokusera sedan på W."

# ../lisp/iconify.jl 
msgid ""
"Toggle the `stickiness' of the window--whether or not it is a member of\n"
"all workspaces."
msgstr ""
"Växla fönstrets 'stickiness', oberoende av om det är en medlem av alla\n"
"skrivbord."

# ../lisp/ignore-window.jl 
msgid "Ignore the window."
msgstr "Ignorera fönstret."

# ../lisp/ignore-window.jl 
msgid "Unignore the window."
msgstr "Ignorera ej fönstret."

# ../lisp/ignore-window.jl 
msgid "Toggle whether a window is ignored or not."
msgstr "Växla mellan om ett fönster är ignorerat eller ej."

# ../lisp/ignore-window.jl 
msgid "Toggle whether a window is focusable or not."
msgstr "Växla mellan om ett fönster är fokuserbart eller ej."

# ../lisp/keymap.jl 
msgid ""
"When nil, the map-keymap function will only map over the first level of\n"
"keymaps, i.e. all prefix keys are ignored."
msgstr ""
"När noll, så kommer map-keymap-funktionen endast binda den första nivån\n"
"av tangentbindningar, d.v.s. alla prefix-tangenter ignoreras."

# ../lisp/keymap.jl 
msgid ""
"Map FUNCTION over all key bindings under the keymap or list of\n"
"keymaps KEYMAP. FUNCTION is called as (FUNCTION KEY), where KEY is a\n"
"cons cell (COMMAND . EVENT)."
msgstr ""
"Bind FUNKTION till alla tangentbordsbindningar i tangentbindningen eller\n"
"listan av tangentbindning KEYMAP. FUNKTION anropas som (FUNKTION TANGENT),\n"
"där tangent är en cons cell (KOMMANDO . HÄNDELSE)."

# ../lisp/keymap.jl 
msgid ""
"Substitute all occurrences of the command OLDDEF for the command NEWDEF\n"
"in the keybindings under the keymap or list of keymaps KEYMAP."
msgstr ""
"Byt ut alla förekomster av kommandot GAMMALDEF mot kommandot NYDEF i\n"
"tangentbindningen eller listan av tangentbindningar KEYMAP."

# ../lisp/keymap.jl 
msgid ""
"Substitute all occurrences of the event OLD-EV for the event NEW-EV\n"
"in the keybindings under the keymap or list of keymaps KEYMAP."
msgstr ""
"Byt ut alla förekomster av händelsen GAMMAL-HÄNDELSE mot händelsen\n"
"NY-HÄNDELSE i tangentbindningen eller listan av tangentbindningar KEYMAP."

# ../lisp/keymap.jl 
msgid ""
"Install the list of BINDINGS in KEYMAP, assuming that KEYMAP is available\n"
"once FEATURE has been provided. If FEATURE has not yet been loaded, arrange\n"
"for the bindings to be installed if and when it is."
msgstr ""
"Installera listan med BINDNINGAR i KEYMAP, under antagande att KEYMAP är\n"
"tillgänglig när FINESS har laddats. Om FINESS ännu ej laddats, ordna så att\n"
"bindningar installeras om och när den laddas."

# ../lisp/keymap.jl 
msgid "Press key..."
msgstr "Tryck tangent..."

# ../lisp/keymaps.jl 
msgid "Keymap containing bindings active anywhere."
msgstr "Tangentbindning vars bindningar är aktive överallt."

# ../lisp/keymaps.jl 
msgid "Keymap containing bindings active when a client window is focused."
msgstr "Tangentbindning vars bindningar är aktiva när ett fönster är fokuserat."

# ../lisp/keymaps.jl 
msgid ""
"Keymap containing bindings active when the pointer is in the root window\n"
"(or when no window is focused)."
msgstr ""
"Tangentbindning vars bindningar är aktiva när pekaren är i rotfönstret\n"
"(eller när inget fönster är fokuserat)."

# ../lisp/keymaps.jl 
msgid ""
"Keymap containing bindings active when the pointer is in the title of\n"
"a window. (Only mouse-bindings are evaluated in this map.)"
msgstr ""
"Tangentbindning vars bindningar är aktiva när pekaren är i ett fönsters\n"
"titelrad. (Endast musbindningar fungerar här.)"

# ../lisp/keymaps.jl 
msgid ""
"Keymap containing bindings active when the pointer is in the border of\n"
"a window. (Only mouse-bindings are evaluated in this map.)"
msgstr ""
"Tangentbindning vars bindningar är aktiva när pekaren är i ett fönsters\n"
"ram. (Endast musbindningar fungerar här.)"

# ../lisp/keymaps.jl 
msgid ""
"Keymap containing bindings active when the pointer is in the close button\n"
"of a window. (Only mouse-bindings are evaluated in this map.)"
msgstr ""
"Tangentbindning vars bindningar är aktiva när pekaren är på ett fönsters\n"
"stängknapp. (Endast musbindningar fungerar här.)"

# ../lisp/keymaps.jl 
msgid ""
"Keymap containing bindings active when the pointer is in the iconify\n"
"button of a window. (Only mouse-bindings are evaluated in this map.)"
msgstr ""
"Tangentbindning vars bindningar är aktiva när pekaren är på ett fönsters\n"
"ikonifieringsknapp. (Endast musbindningar fungerar här.)"

# ../lisp/keymaps.jl 
msgid ""
"Keymap containing bindings active when the pointer is in the maximize\n"
"button of a window. (Only mouse-bindings are evaluated in this map.)"
msgstr ""
"Tangentbindning vars bindningar är aktiva när pekaren är på ett fönsters\n"
"maximeringsknapp. (Endast musbindningar fungerar här.)"

# ../lisp/keymaps.jl 
msgid ""
"Keymap containing bindings active when the pointer is in the menu button\n"
"of a window. (Only mouse-bindings are evaluated in this map.)"
msgstr ""
"Tangentbindning vars bindningar är aktiva när pekaren är på ett fönsters\n"
"menyknapp. (Endast musbindningar fungerar här.)"

# ../lisp/match-window.jl 
msgid "Matched Windows"
msgstr "Matchade fönster"

# ../lisp/match-window.jl 
msgid "Match windows to properties."
msgstr "Matcha fönster mot egenskaper."

# ../lisp/maximize.jl 
msgid ""
"Maximizing a window dimension always increases the size of that dimension."
msgstr ""
"Maximering av en fönsterdimension ökar alltid storleken på den dimensionen."

# ../lisp/maximize.jl 
msgid "Raise windows when they're maximized."
msgstr "Höj fönster när de maximeras."

# ../lisp/maximize.jl 
msgid "Let ignored windows be overlapped when filling windows."
msgstr "Låt ignorerade fönster täckas över när fönster 'fylls'."

# ../lisp/maximize.jl 
msgid "Maximize the dimensions of the window."
msgstr "Maximera fönstrets dimensioner."

# ../lisp/maximize.jl 
msgid ""
"Restore the dimensions of the window to its original, unmaximized, state."
msgstr "Återställ fönstret till dess ursprungliga, icke maximerade läge."

# ../lisp/maximize.jl 
msgid "Maximize the vertical dimension of the window."
msgstr "Maximera fönstrets vertikala dimension."

# ../lisp/maximize.jl 
msgid "Maximize the horizontal dimension of the window."
msgstr "Maximera fönstrets horisontella dimension."

# ../lisp/maximize.jl 
msgid "Toggle the state of the window between maximized and unmaximized."
msgstr "Växla fönstrets läge mellan maximerade och icke maximerad."

# ../lisp/maximize.jl 
msgid ""
"Toggle the state of the window between vertically maximized and\n"
"unmaximized."
msgstr ""
"Växla fönstrets läge mellan vertikalt maximerad och icke maximerad."

# ../lisp/maximize.jl 
msgid ""
"Toggle the state of the window between horizontally maximized and\n"
"unmaximized."
msgstr ""
"Växla fönstrets läge mellan horisontellt maximerad och icke maximerad."

# ../lisp/maximize.jl 
msgid "Maximize the window without obscuring any other windows."
msgstr "Maximera fönster utan att täcka över andra fönster."

# ../lisp/maximize.jl 
msgid "Maximize the window vertically without obscuring any other windows."
msgstr "Maximera fönstret vertikalt utan att täcka över andra fönster."

# ../lisp/maximize.jl 
msgid ""
"Toggle the state of the window between maximized-filled and unmaximized."
msgstr ""
"Växla fönsterläge mellan fyllnadsmaximerad och ej maximerad."

# ../lisp/maximize.jl 
msgid ""
"Toggle the state of the window between vertically maximized-filled and\n"
"unmaximized."
msgstr ""
"Växla fönsterläge mellan vertikalt fyllnadsmaximerad och ej maximerad."

# ../lisp/maximize.jl 
